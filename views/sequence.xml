<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <!-- Sequences for purchase.order -->
        <record id="seq_receive_com" model="ir.sequence">
            <field name="name">Receive Computer</field>
            <field name="code">comservice.com_receive</field>
            <field name="prefix">AS%(year)s-</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_return_com" model="ir.sequence">
            <field name="name">Return Computer</field>
            <field name="code">comservice.com_return</field>
            <field name="prefix">RT%(year)s-</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</openerp>
